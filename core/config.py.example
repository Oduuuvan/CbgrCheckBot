from dataclasses import dataclass


@dataclass
class Config:
    token: str = 'BOT_TOKEN'            # токен вашего бота
    admin_id: int = 000000000           # id администратора бота
    user_id_report: int = 000000000     # id пользователя, которому будут приходить отчеты
    db_folder: str = 'DB_FOLDER/'       # путь к папке, где будет база данных
    csv_folder: str = 'CSV_FOLDER/'     # путь к папке, где временно будут создаваться файлы отчета
    log_level: str = 'ERROR'            # уровень логирования
